{
	"$schema": "http://json-schema.org/draft-04/schema#", 
	"type": "object", 
	"properties": {
		"classes": {
			"type": "array", 
			"items": {
				"type": "object", 
				"properties": {
					"name": {
						"type": "string"
					}, 
					"methods": {
						"type": "array", 
						"items": {
							"type": "string"
						}
					}, 
					"attributes": {
						"type": "array", 
						"items": {
							"type": "string"
						}
					}
				}
			}, 
			"required": [
				"name", 
				"methods", 
				"attributes"
			]
		}, 
		"otherMethods": {
			"type": "array", 
			"items": {
				"type": "string"
			}
		}, 
		"otherAttributes": {
			"type": "array", 
			"items": {
				"type": "string"
			}
		}, 
		"connections": {
			"type": "array", 
			"items": {
				"type": "object", 
				"properties": {
					"type": {
						"enum": [
							"AGGREGATION", 
							"ASSOCIATION", 
							"COMPOSITION", 
							"GENERALIZATION", 
							"IMPLEMENTATION", 
							"STANDARD"
						]
					}, 
					"source": {
						"type": "string"
					}, 
					"target": {
						"type": "string"
					}, 
					"mulstart": {
						"enum": [
							"1", 
							"*", 
							""
						]
					}, 
					"multarget": {
						"enum": [
							"1", 
							"*", 
							""
						]
					}
				}, 
				"required": [
					"type", 
					"source", 
					"target", 
					"mulstart", 
					"multarget"
				]
			}
		}
	}, 
	"required": [
		"classes", 
		"otherMethods", 
		"otherAttributes", 
		"connections"
	]
}