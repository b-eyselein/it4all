{
	"$schema": "http://json-schema.org/draft-04/schema#", 
	"type": "object", 
	"properties": {
		"classes": {
			"type": "array", 
			"items": {
				"type": "object", 
				"properties": {
					"name": {
						"type": "string"
					}, 
					"methods": {
						"type": "array", 
						"items": {
							"type": "string"
						}
					}, 
					"attributes": {
						"type": "array", 
						"items": {
							"type": "string"
						}
					}
				}
			}, 
			"required": [
				"name", 
				"methods", 
				"attributes"
			]
		}, 
		"associations": {
			"type": "array", 
			"items": {
				"type": "object", 
				"properties": {
					"assocType": {
						"enum": [
							"AGGREGATION", 
							"ASSOCIATION", 
							"COMPOSITION"
						]
					}, 
					"start": {
						"type": "object", 
						"properties": {
							"endName": {
								"type": "string"
							}, 
							"multiplicity": {
								"enum": [
									"SINGLE", 
									"UNBOUND"
								]
							}
						}, 
						"required": [
							"endName", 
							"multiplicity"
						]
					}, 
					"target": {
						"type": "object", 
						"properties": {
							"endName": {
								"type": "string"
							}, 
							"multiplicity": {
								"enum": [
									"SINGLE", 
									"UNBOUND"
								]
							}
						}, 
						"required": [
							"endName", 
							"multiplicity"
						]
					}
				}, 
				"required": [
					"assocType", 
					"start", 
					"target"
				]
			}
		}, 
		"implementations": {
			"type": "array", 
			"items": {
				"type": "object", 
				"properties": {
					"subClass": {
						"type": "string"
					}, 
					"superClass": {
						"type": "string"
					}
				}, 
				"required": [
					"subClass", 
					"superClass"
				]
			}
		}
	}, 
	"required": [
		"classes", 
		"associations", 
		"implementations"
	]
}