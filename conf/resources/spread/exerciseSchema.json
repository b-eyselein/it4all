{
	"$schema": "http://json-schema.org/draft-04/schema#", 
	"type": "array", 
	"items": {
		"type": "object", 
		"properties": {
			"id": {
				"type": "integer", 
				"minimum": 1
			}, 
			"title": {
				"type": "string"
			}, 
			"text": {
				"type": "array", 
				"items": {
					"type": "string"
				}
			}, 
			"sampleFileName": {
				"type": "string"
			}, 
			"templateFileName": {
				"type": "string"
			}
		}
	}, 
	"required": [
		"id", 
		"title", 
		"text", 
		"sampleFileName", 
		"tempalteFileName"
	]
}