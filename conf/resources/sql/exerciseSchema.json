{
	"$schema": "http://json-schema.org/draft-04/schema#", 
	"type": "array", 
	"items": {
		"type": "object", 
		"properties": {
			"id": {
				"type": "integer"
			}, 
			"shortName": {
				"type": "string"
			}, 
			"title": {
				"type": "string"
			}, 
			"text": {
				"type": "array", 
				"items": {
					"type": "string"
				}
			}, 
			"scriptFile": {
				"type": "string"
			}, 
			"exercises": {
				"type": "array", 
				"items": {
					"type": "object", 
					"properties": {
						"key": {
							"type": "object", 
							"properties": {
								"exerciseId": {
									"type": "integer"
								}, 
								"scenarioId": {
									"type": "integer"
								}
							}, 
							"required": [
								"exerciseId", 
								"scenarioId"
							]
						}, 
						"author": {
							"type": "string"
						}, 
						"text": {
							"type": "array", 
							"items": {
								"type": "string"
							}
						}, 
						"exerciseType": {
							"enum": [
								"CREATE", 
								"SELECT", 
								"UPDATE", 
								"INSERT", 
								"DELETE"
							]
						}, 
						"hint": {
							"type": "string"
						}, 
						"validation": {
							"type": "string"
						}, 
						"tags": {
							"type": "array", 
							"items": {
								"enum": [
									"JOIN", 
									"DOUBLE_JOIN", 
									"TRIPLE_JOIN", 
									"ORDER_BY", 
									"GROUP_BY", 
									"FUNCTION", 
									"ALIAS", 
									"LIMIT", 
									"SUBSELECT"
								]
							}
						}, 
						"samples": {
							"type": "array", 
							"items": {
								"type": "object", 
								"properties": {
									"key": {
										"type": "object", 
										"properties": {
											"scenarioId": {
												"type": "integer"
											}, 
											"exerciseId": {
												"type": "integer"
											}, 
											"sampleId": {
												"type": "integer"
											}
										}, 
										"required": [
											"scenarioId", 
											"exerciseId", 
											"sampleId"
										]
									}, 
									"sample": {
										"type": "array", 
										"items": {
											"type": "string"
										}
									}
								}, 
								"required": [
									"key", 
									"sample"
								]
							}
						}
					}, 
					"required": [
						"key", 
						"exerciseType", 
						"text", 
						"hint", 
						"samples", 
						"validation", 
						"tags"
					]
				}
			}
		}, 
		"required": [
			"shortName", 
			"title", 
			"scriptFile", 
			"exercises"
		]
	}, 
	"minItems": 1, 
	"maxItems": 1
}