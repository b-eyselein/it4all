{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"id": {
				"type": "integer"
			},
			"title": {
				"type": "string"
			},
			"author": {
				"type": "string"
			},
			"text": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"inputCount": {
				"type": "integer"
			},
			"functionName": {
				"type": "string"
			},
			"samples": {
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"key": {
							"type": "object",
							"properties": {
								"exerciseId": {
									"type": "integer"
								},
								"language": {
									"enum": [
										"PYTHON_3",
										"JAVA"
									]
								}
							},
							"required": [
								"exerciseId",
								"language"
							],
							"additionalProperties": false
						},
						"sample": {
							"type": "string"
						}
					},
					"required": [
						"key",
						"sample"
					],
					"additionalProperties": false
				}
			},
			"sampleTestData": {
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"key": {
							"type": "object",
							"properties": {
								"testId": {
									"type": "integer"
								},
								"exerciseId": {
									"type": "integer"
								}
							},
							"required": [
								"testId",
								"exerciseId"
							],
							"additionalProperties": false
						},
						"inputs": {
							"type": "string"
						},
						"output": {
						}
					},
					"required": [
						"key",
						"inputs",
						"output"
					],
					"additionalProperties": false
				}
			}
		},
		"required": [
			"id",
			"title",
			"author",
			"text",
			"functionName",
			"inputCount",
			"samples",
			"sampleTestData"
		],
		"additionalProperties": false
	}
}
