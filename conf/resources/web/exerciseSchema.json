{
	"$schema": "http://json-schema.org/draft-04/schema#", 
	"type": "array", 
	"items": {
		"type": "object", 
		"properties": {
			"id": {
				"type": "integer", 
				"minimum": 1
			}, 
			"title": {
				"type": "string"
			}, 
			"text": {
				"type": "string"
			}, 
			"htmlText": {
				"type": "string"
			}, 
			"cssText": {
				"type": "string"
			}, 
			"jsText": {
				"type": "string"
			}, 
			"htmlTasks": {
				"type": "array", 
				"items": {
					"type": "object", 
					"properties": {
						"text": {
							"type": "string"
						}, 
						"xpath": {
							"type": "string"
						}, 
						"attributes": {
							"type": "array", 
							"items": {
								"type": "object", 
								"properties": {
									"key": {
										"type": "string"
									}, 
									"value": {
										"type": "string"
									}
								}
							}
						}, 
						"content": {
							"type": "string"
						}
					}, 
					"required": [
						"text", 
						"xpath"
					]
				}
			}, 
			"cssTasks": {
				"type": "array", 
				"items": {
					"type": "object", 
					"properties": {
						"text": {
							"type": "string"
						}, 
						"xpath": {
							"type": "string"
						}, 
						"attributes": {
							"type": "array", 
							"items": {
								"type": "object", 
								"properties": {
									"key": {
										"type": "string"
									}, 
									"value": {
										"type": "string"
									}
								}
							}
						}
					}, 
					"required": [
						"text", 
						"xpath"
					]
				}
			}, 
			"jsTasks": {
				"type": "array", 
				"items": {
					"type": "object", 
					"properties": {
						"text": {
							"type": "string"
						}, 
						"xpath": {
							"type": "string"
						}, 
						"conditions": {
							"type": "array", 
							"items": {
								"type": "object", 
								"properties": {
									"id": {
										"type": "integer"
									}, 
									"xpath": {
										"type": "string"
									}, 
									"awaitedValue": {
										"type": "string"
									}, 
									"isPrecondition": {
										"type": "boolean"
									}
								}, 
								"required": [
									"id", 
									"xpath", 
									"awaitedValue", 
									"isPrecondition"
								]
							}
						}, 
						"action": {
							"type": "object", 
							"properties": {
								"actionxpath": {
									"type": "string"
								}, 
								"actiontype": {
									"enum": [
										"CLICK", 
										"FILLOUT"
									]
								}, 
								"keystosend": {
									"type": "string"
								}
							}, 
							"required": [
								"actionxpath", 
								"actiontype"
							]
						}
					}, 
					"required": [
						"text", 
						"xpath", 
						"conditions", 
						"action"
					]
				}
			}
		}, 
		"required": [
			"id", 
			"title", 
			"text", 
			"htmlTasks"
		]
	}, 
	"minItems": 1, 
	"uniqueItems": true
}