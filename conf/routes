# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET    /               controllers.Application.index()

# Userverwaltung
->     /               core.Routes

# User
GET    /user           controllers.UserController.index()

# Python Test
GET    /python         controllers.Application.pythonTest()

# Binary and Boolean
->     /binary         binary.Routes

# Web-Korrektur
->     /web            web.Routes

# SQL-Korrektur
->     /sql            sql.Routes

# Mindmap-Korrektur
->     /mindmap        mindmap.Routes

# XML
->     /xml            xml.Routes

# Excel
->     /spread         spread.Routes

# UML
->     /uml            uml.Routes

# Evaluation
GET    /eval/          controllers.Evaluation.index()
POST   /eval/submit    controllers.Evaluation.submit()

# Map static resources from the /public folder to the /assets URL path
GET    /assets/*file   controllers.Assets.versioned(path="/public", file: Asset)
